{
	"builds": [
        {
            "src": "web/maganghub/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12"
            }
        }
    ],
	"routes": [
		{
			"src": "web/static/(.*)",
			"dest": "web/staticfiles/$1"
		},
		{
			"src": "/(.*)",
			"dest": "web/maganghub/wsgi.py"
		}
	]
}